{
  "name": "truetype",
  "version": "1.2.0",
  "description": "Simple JS module to check types more concisely",
  "main": "truetype.umd.js",
  "jsnext:main": "truetype.js",
  "author": "Alex Bruno <web.alexbr@gmail.com>",
  "license": "MIT",
  "keywords": [
    "javascript",
    "type",
    "test",
    "check"
  ],
  "homepage": "https://github.com/alexbruno/truetype#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexbruno/truetype.git"
  },
  "bugs": {
    "url": "https://github.com/alexbruno/truetype/issues"
  },
  "scripts": {

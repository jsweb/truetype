{
  "files": [
    "./src/module.ts"
  ],
  "compilerOptions": {
    "outDir": "./dist",
    "target": "esnext",
    "declaration": true,
    "removeComments": false,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
  }
}
